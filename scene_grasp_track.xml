<mujoco model="scene">
    <compiler angle="radian" autolimits="true"/>
    <option timestep="0.002" integrator="implicitfast" solver="Newton" gravity="0 0 -9.82" cone="elliptic"/>
    <visual>
        <global azimuth="120" elevation="-20"/>
        <!--
 <headlight diffuse="0.6 0.6 0.6" specular="0 0 0" /> 
-->
        <rgba haze="0.15 0.25 0.35 1"/>
        <!-- <quality offsamples="5"/> -->
    </visual>
    <statistic meansize="0.1" extent="1" center="4 0 0"/>
    <include file="assets/UR5eGripper/ur5e2f85.xml"/>
    <asset>
        <mesh name="table_mesh" file="assets/table/sigmund_table.stl"/>
        <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
        <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3" markrgb="0.8 0.8 0.8" width="300" height="300"/>
        <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
    </asset>
    <worldbody>
        <!--
 <geom name="floor" size="0 0 0.05" type="plane" /> 
-->
        <geom name="floor" pos="0 0 -0.74" size="0 0 0.05" type="plane" material="groundplane"/>
        <light pos="0 0 0.5" dir="0 0 -1" type="directional" range="100" bulbradius="0.1"/>
        <!-- Tables -->
        <geom name="table_1" pos="0.801 -0.6 0" type="mesh" rgba="0.1 0.1 0.1 1" mesh="table_mesh" condim="6" solref="0.001 2" solimp="0.99 0.999 0.001"/>
        <geom name="table_2" pos="0 -0.6 0" type="mesh" rgba="0.1 0.1 0.1 1" mesh="table_mesh" condim="6" solref="0.001 2" solimp="0.99 0.999 0.001"/>
        <!-- objects -->
        <body name="box1" pos="0.6 0.3 0.03" quat="1 0 0 0">
            <freejoint name="box1"/>
            <geom name="box1" type="box" mass="0.01" size="0.03 0.03 0.03" rgba="1 0 0 1" condim="6" solref="0.001 2" solimp="0.99 0.999 0.001"/>
        </body>
        <body name="box2" pos="0.6 0.2 0.03" quat="1 0 0 0">
            <freejoint name="box2"/>
            <geom name="box2" type="box" mass="0.01" size="0.03 0.03 0.03" rgba="1 0 0 1" condim="6" solref="0.001 2" solimp="0.99 0.999 0.001"/>
        </body>
        <body name="box3" pos="0.6 0.1 0.03" quat="1 0 0 0">
            <freejoint name="box3"/>
            <geom name="box3" type="box" mass="0.01" size="0.03 0.03 0.03" rgba="1 0 0 1" condim="6" solref="0.001 2" solimp="0.99 0.999 0.001"/>
        </body>
        <body name="box4" pos="0.6 0.0 0.03" quat="1 0 0 0">
            <freejoint name="box4"/>
            <geom name="box4" type="box" mass="0.01" size="0.03 0.03 0.03" rgba="0 1 0 1" condim="6" solref="0.001 2" solimp="0.99 0.999 0.001"/>
        </body>
        <body name="box5" pos="0.6 -0.1 0.03" quat="1 0 0 0">
            <freejoint name="box5"/>
            <geom name="box5" type="box" mass="0.01" size="0.03 0.03 0.03" rgba="0 1 0 1" condim="6" solref="0.001 2" solimp="0.99 0.999 0.001"/>
        </body>
        <body name="box6" pos="0.6 -0.2 0.03" quat="1 0 0 0">
            <freejoint name="box6"/>
            <geom name="box6" type="box" mass="0.01" size="0.03 0.03 0.03" rgba="0 0 1 1" condim="6" solref="0.001 2" solimp="0.99 0.999 0.001"/>
        </body>
        <!--Zones-->
        <body name="zone_pickup" pos="0.6 0 -0.01" quat="1 0 0 0">
            <geom name="zone_pickup" type="box" size="0.2 0.6 0.01" rgba="0 1 0 0.1" contype="0" conaffinity="0"/>
        </body>
        <body name="zone_drop" pos="-0.6 0 -0.01" quat="1 0 0 0">
            <geom name="zone_drop" type="box" size="0.2 0.6 0.01" rgba="1 0 0 0.1" contype="0" conaffinity="0"/>
        </body>
        <!-- axises -->
        <!--
 <geom name="x-axis" group="2" type="cylinder" contype="0" conaffinity="0" fromto="0 0 0 1 0 0" size="0.01" rgba="1 0 0 1" />
		<geom name="y-axis" group="2" type="cylinder" contype="0" conaffinity="0" fromto="0 0 0 0 1 0" size="0.01" rgba="0 1 0 1" />
		<geom name="z-axis" group="2" type="cylinder" contype="0" conaffinity="0" fromto="0 0 0 0 0 1" size="0.01" rgba="0 0 1 1" /> 
-->
        <body name="pickup_point" pos="0.6 0.3 0.3" quat="1 0 0 0">
            <geom name="pickup_point" type="sphere" size="0.005" rgba="0 0 1 0.5" contype="0" conaffinity="0"/>
        </body>
        <body name="drop_point_box1" pos="-0.6 0.3 0.3" quat="1 0 0 0">
            <geom name="drop_point_box1" type="sphere" size="0.005" rgba="0 0 1 0.5" contype="0" conaffinity="0"/>
        </body>
        <body name="drop_point_box2" pos="-0.6 0 0.3" quat="1 0 0 0">
            <geom name="drop_point_box2" type="sphere" size="0.005" rgba="0 0 1 0.5" contype="0" conaffinity="0"/>
        </body>
        <body name="drop_point_box3" pos="-0.6 -0.3 0.3" quat="1 0 0 0">
            <geom name="drop_point_box3" type="sphere" size="0.005" rgba="0 0 1 0.5" contype="0" conaffinity="0"/>
        </body>
        <camera name="cam1" pos="1.25 0 1.5" euler="0 0.45 1.57" mode="fixed"/>
    </worldbody>
</mujoco>